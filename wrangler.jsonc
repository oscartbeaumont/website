{
  "$schema": "./node_modules/wrangler/config-schema.json",
	"name": "website",
	"main": "@solidjs/start/server/entry",
	"compatibility_date": "2025-10-11",
 "compatibility_flags": [
    "nodejs_compat"
  ],
  "route": {
     "pattern": "demo.otbeaumont.me/*",
     "zone_name": "otbeaumont.me"
  },
	"observability": {
		"enabled": true
	},
	"kv_namespaces": [
    {
      "binding": "KV"
    }
  ]
}
