{
	"$schema": "./node_modules/wrangler/config-schema.json",
	"name": "website",
	"main": "@solidjs/start/server/entry",
	"compatibility_date": "2025-10-11",
	"compatibility_flags": ["nodejs_compat"],
	"route": {
		"pattern": "otbeaumont.me",
		"custom_domain": true
	},
	"observability": {
		"enabled": true
	},
	"kv_namespaces": [
		{
			"binding": "KV"
		}
	],
	"r2_buckets": [
		{
			"binding": "DATA",
			"bucket_name": "website-data"
		}
	]
}
